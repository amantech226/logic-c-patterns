#include <stdio.h>

int main()
{
    int i=0,j=0;
    
    for(i=0;i<9;i++)
    {
        for(j=0;j<9;j++)
        {
           if(i==0 || j==0 || j==8 ||i==8)
           {
               printf("*");
           }
           
           else
           {
               printf(" ");
           }
           
        }
        
        printf("\n");
    }
    
   return 0;
}

